function calculateSum(a, b) {
  return a + b;
}

// Define two numbers
var num1 = 5;
var num2 = 7;

// Calculate the sum using the function
var sum = calculateSum(num1, num2);

// Print the sum
console.log("The sum of " + num1 + " and " + num2 + " is: " + sum);

